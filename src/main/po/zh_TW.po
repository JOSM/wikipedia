msgstr "[Wiki] "

msgid "aliases"
msgstr "別名"

msgid "all Wikipedia articles in Category"
msgstr "分類内所有維基百科條目"

msgid "clipboard"
msgstr "剪貼簿"

msgid "coordinates"
msgstr "座標"

msgid "description"
msgstr "描述"

msgid "has no Q-ID"
msgstr "沒有 Q-ID"

msgid "help"
msgstr "說明"

