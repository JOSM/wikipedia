msgstr[0] ""
"Substituir valor na etiqueta ''{0}'' {1} em {2} pelo novo valor ''{3}''?"
msgstr[1] ""
"Substituir valores na etiqueta ''{0}'' {1} em {2} pelo novo valor ''{3}''?"

msgid "Overwrite key"
msgstr "Substituir chave"

